{
  "name": "mock-workspace",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "j": "npx -y jiti src/cli.ts",
    "j:alias": "JITI_ALIAS='{\"@utils\": \"../src/utils\"}' npx -y jiti src/cli-import-path-alias.ts",
    "tsj": "npx -y ts-jiti --tsconfig tsconfig.json src/cli.ts",
    "tsj:alias": "npx -y ts-jiti --tsconfig tsconfig.json src/cli-import-path-alias.ts",
    "tsj:import": "npx -y ts-jiti --tsconfig tsconfig.json src/cli-load-import.ts src/utils/string.ts"
  },
  "dependencies": {
    "jiti": ">2.4.1"
  }
}
